{
	"name": "create-migration",
	"displayName": "Create Migration",
	"description": "Create Migration with db-migrate create",
	"version": "0.0.1",
	"publisher": "Ivanov-Artyom",
	"icon": "images/icon.jpg",
	"engines": {
		"vscode": "^1.34.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:samples.BoxInput"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
		  "type": "object",
		  "title": "create-migration",
		  "properties": {
		  "create-migration.autoRun": {
			  "type": "boolean",
			  "default": false,
			  "description": ""
			},
			"create-migration.env": {
				"scope": "resource",
				"type": "string",
				"default": "localdev32",
				"description": ""
			},
			"create-migration.prefixTask": {
				"scope": "resource",
				"type": "string",
				"default": "LW-",
				"description": ""
			}
		  }
		}
	  },
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "tslint -p ./",
		"watch": "tsc -watch -p ./"
	},
	"homepage": "https://github.com/ArtyomSleepNeeded/vscode-create-migration",
	"repository": {
		"type": "git",
		"url": "https://github.com/ArtyomSleepNeeded/vscode-create-migration.git"
	},
	"devDependencies": {
		"@types/node": "10.3.6",
		"@types/vscode": "^1.35.0",
		"db-migrate-base": "^2.0.0",
		"db-migrate-mysql": "^1.1.10",
		"db-migrate-pg": "^0.2.5",
		"js-polyfills": "^0.1.42",
		"ts-loader": "^6.0.4",
		"tslint": "^5.18.0",
		"typescript": "^3.5.2",
		"vscode": "^1.1.6"
	},
	"dependencies": {
		"db-migrate": "^0.10.7"
	}
}
